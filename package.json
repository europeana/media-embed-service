{
  "name": "media-embed-service",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "webpack-cli",
    "lint": "npx eslint src/",
    "lint:fix": "npx eslint --fix src/",
    "lint:style": "npx stylelint src/**/*.scss",
    "lint:style:fix": "npm run lint:style -- --fix",
    "start:dev": "NODE_ENV=development webpack-dev-server",
    "test": "karma start",
    "test:coverage": "npm run test -- --coverage",
    "test:e2e": "npm run test:e2e:chrome",
    "test:e2e:chrome": "node nightwatch-runner.js --env=chrome",
    "test:e2e:firefox": "node nightwatch-runner.js --env=gecko --skiptags skip-firefox",
    "test:e2e:chrome:headless": "npm run test:e2e:chrome -- --headless --skiptags skip-chrome-headless",
    "test:e2e:firefox:headless": "npm run test:e2e:firefox -- --headless",
    "test:e2e:headless": "npm run test:e2e:firefox:headless && npm run test:e2e:chrome:headless",
    "test:e2e:non-headless": "npm run test:e2e:firefox && npm run test:e2e",
    "test:e2e:all": "npm run test:e2e:non-headless && npm run test:e2e:headless"
  },
  "dependencies": {
    "@europeana/media-player": "^0.7.3",
    "dotenv": "^8.2.0",
    "materialize-css": "^1.0.0",
    "placeload.js": "^1.0.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "axe-core": "^3.5.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "chromedriver": "83.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^6.8.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^4.2.0",
    "geckodriver": "1.19.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-ajax": "^4.0.0",
    "jquery": "^3.4.1",
    "karma-coverage": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "mini-css-extract-plugin": "^0.9.0",
    "nightwatch": "1.3.4",
    "nightwatch-accessibility": "^1.8.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.23.0",
    "stylelint": "^13.3.2",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.1.0",
    "webpack-karma-jasmine": "^3.0.8"
  },
  "resolutions": {
    "**/**/braces": "^2.3.1",
    "**/**/serialize-javascript": "^2.1.1"
  }
}
